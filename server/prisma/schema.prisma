// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id String @id
  login String @unique
  name String
  salary Float
  createdAt DateTime
}

model UploadStatus {
  uploading Boolean @id
  owner String
  updatedAt DateTime @updatedAt
}